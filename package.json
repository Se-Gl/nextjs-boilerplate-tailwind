{
  "name": "nextjs-boilerplate-tailwind",
  "version": "0.0.0-semantic-release",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "cypress:open": "cypress open --config-file=cypress.config.js",
    "cypress:run": "cypress run",
    "prepare": "husky install",
    "prettier:check": "prettier --check .",
    "prettier:format": "prettier --write .",
    "success-message": "echo 🚀 Everything is awesome. 🚀...🖥️  Auto checking completed, ready to deploy.🖥️...☁️  Run: git push ☁️ \n\n\n\n",
    "pre-commit": "npm run cypress:run && npm run prettier:format && npm run prettier:check && npm run lint && npm run build && git add -A . && npm run success-message"
  },
  "dependencies": {
    "next": "^13.4.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "autoprefixer": "^10.4.14",
    "cypress": "^12.11.0",
    "eslint": "^8.40.0",
    "eslint-config-next": "^13.4.1",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "semantic-release": "^21.0.2",
    "tailwindcss": "^3.3.2"
  }
}
